# Crawlify Environment Variables Example
# Copy this file to .env and configure the values according to your needs

# OpenRouter API key for AI-powered summaries
OPENROUTER_API_KEY=your_openrouter_key_here

# HTTP proxy for crawling requests (optional)
CRAWL_HTTP_PROXY=

# Search backend: searxng(default) or serpapi
SEARCH_BACKEND=searxng
SEARXNG_BASE_URL=http://searxng:8080
SEARXNG_CATEGORIES=general
SEARXNG_LANGUAGE=en
SEARXNG_ENGINES=

# SerpAPI API key (required if using serpapi backend)
SERPAPI_API_KEY=
